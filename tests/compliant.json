      "type": "aws_iam_user",
      "change": {
        "after": {
          "name": "hipaa-user-john-doe",
          "path": "/hipaa-users/",
          "tags": {
            "Purpose": "HIPAA Compliance",
            "Control": "164.312(a)(1)",
            "Environment": "prod"
          }
        }
      }
